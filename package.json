{
  "name": "cryptowatch-embed",
  "version": "0.3.2",
  "description": "Client library for configuring and embedding Cryptowatch charts",
  "main": "dist/main.js",
  "author": "Artur Sapek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptowatch/embed.git"
  },
  "scripts": {
